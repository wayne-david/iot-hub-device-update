{
    "_1": "Documentation:",
    "_2": "https://pypi.org/project/cmake_format/",
    "_3": " Usage:",
    "_4": "find . -name CMakeLists.txt -print0 | xargs -0 -n1 -P4 cmake-format --in-place",
    "line_width": 100,
    "tab_size": 4,
    "max_subgroups_hwrap": 2,
    "max_pargs_hwrap": 3,
    "separate_ctrl_name_with_space": true,
    "separate_fn_name_with_space": true,
    "dangle_parens": false,
    "line_ending": "windows",
    "command_case": "lower",
    "keyword_case": "upper",
    "enable_markup": false
}
